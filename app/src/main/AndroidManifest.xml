<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mymiwok">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.android.mymiwok.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.android.mymiwok.NumbersActivity"
            android:label="@string/category_numbers">

        </activity>
        <activity android:name="com.example.android.mymiwok.PhraseActivity"
            android:label="@string/category_phrases"/>
        <activity android:name="com.example.android.mymiwok.FamilyActivity"
            android:label="@string/category_family"/>
        <activity android:name="com.example.android.mymiwok.ColorsActivity"
            android:label="@string/category_colors" />
    </application>

</manifest>